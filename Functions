#frase = 'como surgiu a filosofia na Grécia'
teste = input("digite: ").split()
lista = ['surgiu', 'filosofia']
def wordInFrase(teste, lista):
	for plv1 in lista:
		if plv1 not in teste:
			return False
	return True
	
print(wordInFrase(teste, lista))


